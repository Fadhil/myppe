{
  "name": "ppe-crisis-api",
  "version": "1.0.0",
  "description": "A backend api service for admin panel, frontend users & pharmacy admin",
  "main": "index.js",
  "repository": "git@bitbucket.org-raghu_kumar:machineintelligence7/ppe-crisis-api.git",
  "author": "Raghuram Kumar <raghuk46@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn run clean && yarn run build-server && yarn run copy-node-modules",
    "build-server": "babel src -d dist --source-maps --copy-files --ignore node_modules",
    "clean": "rm -rf ./dist",
    "copy-node-modules": "rm -rf ./node_modules && yarn install --production && cp -R ./node_modules ./dist",
    "start": "NODE_ENV=production node ./dist/server.js",
    "start:local": "cross-env NODE_ENV=local nodemon  ./src/server.js --exec babel-node --watch src"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "query-string": "^6.11.1",
    "sequelize": "^5.21.6",
    "serverless-http": "^2.3.2",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/plugin-transform-classes": "^7.9.2",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-jest": "^25.2.6",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-environment-node": "^25.2.6",
    "lint-staged": "^10.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.3",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "validate-git-commit-message": "^2.0.1"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.653.0"
  }
}
